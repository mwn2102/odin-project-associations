<h1>User show page for <%=@user.name%> </h1>

<h2> User Created Events: </h2>

<h3>Created events: </h3>
<ul>
<%@user.created_events.each do |event| %>
<li><%=event.date%> - <%=event.description%> </li>
<%end%>
</ul>


<p> Upcoming events: </p>
<ul>
<%@upcoming_events.each do |event| %>
<li><%=event.date%> - <%=event.description%> </li>
<%end%>
</ul>

<p> Previous events: </p>
<ul>
<%@prev_events.each do |event| %>
<li><%=event.date%> - <%=event.description%> </li>
<%end%>
</ul>

